## Aliases
alias v='vagrant'
alias vms='VBoxManage list runningvms'

## Bundler
alias b='bundle-hack'
alias be='bundle-exec-hack'
alias bl='bundle install --local'
alias r='rails'

## git
alias g='git'
complete -F _complete_alias g

alias metame='CLOUD_URL=https://cloud-meta.engineyard.com ey'
alias m='CLOUD_URL=https://cloud-meta.engineyard.com ey ssh -e'
alias tfl='tail -n 30 -qF log/*.events.log log/*.actionview.log log/{test,development}.log log/*.controller.log | egrep -v "(Datamapper bug|Cache read)"'

alias wtf='echo "❨╯°□°❩╯︵┻━┻"'
alias idk='echo "¯\_(ツ)_/¯"'

alias reado="ssh-reagent $*"
